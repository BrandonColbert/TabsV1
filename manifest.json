{
	"name": "Tabs+",
	"version": "0.1.0",
	"description": "Extends Chrome's tab management capabilities",
	"permissions": [
		"activeTab",
		"contextMenus",
		"storage",
		"tabs"
	],
	"web_accessible_resources": [
		"divider*"
	],
	"background": {
		"scripts": ["tabs.js"],
		"presistent": false
	},
	"browser_action": {
		"default_title": "Tabs!",
		"default_popup": "menu.html",
		"default_icon": {
			"16": "images/icon16.png",
			"32": "images/icon32.png",
			"48": "images/icon48.png",
			"128": "images/icon128.png"
		}
	},
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"manifest_version": 2
}